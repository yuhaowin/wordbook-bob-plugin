{
  "identifier": "wordbook-bob-plugin",
  "version": "0.1.10",
  "category": "translate",
  "name": "单词本插件",
  "summary": "word book plugin, add word to youdao/eudic/shanday word book",
  "icon": "",
  "author": "Eric_余浩",
  "homepage": "https://blog.yuhaowin.com/2021-01-03-wordbook-bob-plugin",
  "appcast": "https://raw.githubusercontent.com/yuhaowin/wordbook-bob-plugin/main/appcast.json",
  "minBobVersion": "0.5.4",
  "options": [
    {
      "identifier": "selectDict",
      "type": "menu",
      "title": "选择词典",
      "defaultValue": "1",
      "menuValues": [
        {
          "title": "有道词典",
          "value": "1"
        },
        {
          "title": "欧路词典",
          "value": "2"
        },
        {
          "title": "扇贝单词",
          "value": "3"
        }
      ]
    },
    {
      "identifier": "authorization",
      "type": "text",
      "title": "认证信息"
    },
    {
      "identifier": "wordbookId",
      "type": "text",
      "title": "单词本ID"
    },
    {
      "identifier": "onDemand",
      "type": "menu",
      "title": "按须添加",
      "defaultValue": "0",
      "menuValues": [
        {
          "title": "关闭",
          "value": "0"
        },
        {
          "title": "开启",
          "value": "1"
        }
      ]
    },
    {
      "identifier": "endChar",
      "type": "text",
      "title": "结束的字符"
    }
  ]
}
